{
    "entrypoint": "index.html",
    "shell": "src/aptcopilot-app.html",
    "fragments": [
      "src/aptcopilot-home.html",
      "src/aptcopilot-properties.html",
      "src/aptcopilot-contact.html",
      "src/my-view404.html"
    ],
    "sources": [
      "src/**/*",
      "images/**/*"
    ],
    "extraDependencies": [
      "manifest.json",
      "bower_components/webcomponentsjs/*.js"
    ],
    "lint": {
      "rules": ["polymer-2"]
    },
    "builds": [
      {
        "preset": "es5-bundled"
      },
      {
        "preset": "es6-bundled"
      },
      {
        "preset": "es6-unbundled"
      }
    ]
  }
  